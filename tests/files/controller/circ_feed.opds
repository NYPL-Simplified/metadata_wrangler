      <link href="http://localhost:6500/okay/works/contributor/Kathleen%20Tudor/eng/Adult%2CAdults+Only%2CChildren%2CYoung+Adult" type="application/atom+xml;profile=opds-catalog;kind=acquisition" rel="contributor" title="Kathleen Tudor"/>
